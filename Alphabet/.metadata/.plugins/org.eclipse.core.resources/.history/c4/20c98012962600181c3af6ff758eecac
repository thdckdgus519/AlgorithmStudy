import java.util.Scanner;

public class Main 
{
	static char graph[][];

	public static void main(String args[])
	{
		Scanner sc = new Scanner(System.in);
		int y = sc.nextInt();
		int x = sc.nextInt();
		graph = new char[y][x];
		
		for(int i = 0 ; i < y ; i++)
		{
			String input = sc.next();
			for(int j = 0 ; j < x ; j++)
			{
				graph[i][j] = input.charAt(j);
			}
		}
		System.out.println((int)graph[0][1]);
	}
}
