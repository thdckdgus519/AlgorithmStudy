import java.util.Scanner;

public class Main 
{
	static int maze[][];
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		
		int start = sc.nextInt();
		int end = sc.nextInt();
		
		maze = new int[start][end];
		
		for(int i = 0 ; i < start ; i++)
		{
			for(int j = 0 ; j < end ; j++)
			{
				maze[i][j] = 1;
			}
		}
		
		for(int i = 0 ; i < start ; i++)
		{
			for(int j = 0 ; j < end ; j++)
			{
				System.out.print(maze[i][j]);
			}
			System.out.println();
		}
	}
}
